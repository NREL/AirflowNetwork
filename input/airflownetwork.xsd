<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:afn="http://github.com/jasondegraw/AirflowNetwork" targetNamespace="http://github.com/jasondegraw/AirflowNetwork" elementFormDefault="qualified">
    <xs:annotation>
        <xs:documentation>AirflowNetwork Schema - Version 1.1 (April 19, 2019)</xs:documentation>
    </xs:annotation>
    <xs:element name="AirflowNetwork">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Description" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Description of the model</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Elements" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>The set of all airflow elements used in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="PowerLaw" type="afn:PowerLawType" minOccurs="0" maxOccurs="unbounded"/>
                            <xs:element name="DetailedOpening" type="afn:ComisDetailedOpeningType" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Materials" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all materials used in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Description" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Description of the material</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Material" type="afn:MaterialType" minOccurs="1" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Filters" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all filters in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:choice maxOccurs="unbounded">
                            <xs:element name="SimpleFilter" type="afn:SimpleFilterType" maxOccurs="unbounded"/>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
                <xs:element name="DimensionlessSchedules" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all schedules in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DimensionlessSchedule" type="afn:TimeSeriesType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MassSchedules" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all schedules in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="MassSchedule" type="afn:MassTimeSeriesType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MassFlowSchedules" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all schedules in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="MassFlowSchedule" type="afn:MassFlowTimeSeriesType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="SourcesAndSinks">
                    <xs:annotation>
                        <xs:documentation>The set of all sources and sinks in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="ScheduledSource" type="afn:ScheduledSourceType" maxOccurs="unbounded"/>
                            <xs:element name="ConstantCoefficientSourceSink" type="afn:ConstantCoefficentSourceSinkType"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <!-- Building-Specific Elements -->
                <xs:element name="Levels" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all levels in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Level" type="afn:LevelType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Nodes" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all nodes in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Node" type="afn:NodeType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Links" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The set of all links that connect nodes in the model</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Link" type="afn:LinkType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="FlowResults" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Flow results by link</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="FlowResult" type="afn:FlowResultType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="PressureResults" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Pressure results by node</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="PressureResult" type="afn:PressureResultType" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
        <!-- Constraints -->
        <xs:key name="ElementKey">
            <xs:selector xpath="afn:Elements/*"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="LinkElementKeyRef" refer="afn:ElementKey">
            <xs:selector xpath="afn:Links/afn:Link/afn:ElementID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="FilterKey">
            <xs:selector xpath="afn:Filters/*"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="LinkFilterKeyRef" refer="afn:FilterKey">
            <xs:selector xpath="afn:Links/afn:Link/afn:FilterID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="MaterialKey">
            <xs:selector xpath="afn:Materials/afn:Material"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="SimpleFilterRemovalMaterialKeyRef" refer="afn:MaterialKey">
            <xs:selector xpath="afn:Filters/afn:SimpleFilter/afn:RemovalEfficiencies/afn:RemovalEfficiency"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:keyref name="ScheduledSourceMaterialKeyRef" refer="afn:MaterialKey">
            <xs:selector xpath="afn:SourcesAndSinks/afn:ScheduledSource/afn:MaterialID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="NodeKey">
            <xs:selector xpath="afn:Nodes/afn:Node"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="NodeKeyRef" refer="afn:NodeKey">
            <xs:selector xpath="afn:Links/afn:Link/afn:Nodes/afn:Node/afn:NodeID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="LevelKey">
            <xs:selector xpath="afn:Levels/afn:Level"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="NodeLevelKeyRef" refer="afn:LevelKey">
            <xs:selector xpath="afn:Nodes/afn:Node/afn:LevelID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="DimensionlessScheduleKey">
            <xs:selector xpath="afn:DimensionlessSchedules/afn:DimensionlessSchedule"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="LinkFilterScheduleKeyRef" refer="afn:DimensionlessScheduleKey">
            <xs:selector xpath="afn:Links/afn:Link/afn:FilterScheduleID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="MassScheduleKey">
            <xs:selector xpath="afn:MassSchedules/afn:MassSchedule"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="ScheduledSourceMassScheduleKeyRef" refer="afn:MassScheduleKey">
            <xs:selector xpath="afn:SourcesAndSinks/afn:ScheduledSource/afn:MassScheduleID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
        <xs:key name="MassFlowScheduleKey">
            <xs:selector xpath="afn:MassFlowSchedules/afn:MassFlowSchedule"/>
            <xs:field xpath="@ID"/>
        </xs:key>
        <xs:keyref name="ScheduledSourceMassFlowScheduleKeyRef" refer="afn:MassFlowScheduleKey">
            <xs:selector xpath="afn:SourcesAndSinks/afn:ScheduledSource/afn:MassFlowScheduleID"/>
            <xs:field xpath="@IDref"/>
        </xs:keyref>
    </xs:element>
    <!-- Flow Elements -->
    <xs:complexType name="PowerLawType">
        <xs:annotation>
            <xs:documentation>Power law flow element</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Description" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of the power law flow element</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Formulation" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="AIRNET"/>
                        <xs:enumeration value="CONTAM"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="FlowBasis">
                <xs:annotation>
                    <xs:documentation>Basis upon which flow is calculated</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Mass"/>
                        <xs:enumeration value="Volume"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Coefficient">
                <xs:annotation>
                    <xs:documentation>Power law flow coefficient</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minExclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="LaminarCoefficient" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Power law flow coefficient for small pressure differences, defaults to the coefficient value</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minExclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Exponent">
                <xs:annotation>
                    <xs:documentation>Power law flow exponent</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="0.5"/>
                        <xs:maxInclusive value="1.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ReferenceState" type="afn:AirStateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Reference state of the power law element</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Transition">
                <xs:annotation>
                    <xs:documentation>Specify how small pressure differences are to be handled</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="ByMinimum" type="xs:string" fixed="">
                            <xs:annotation>
                                <xs:documentation>Power law flow transition by minimum flow from laminar and turbulent calculations</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ReynoldsNumber">
                            <xs:annotation>
                                <xs:documentation>Power law flow transition Reynolds number</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:double">
                                    <xs:minExclusive value="0.0"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="PressureDifference" type="afn:PressureQuantityType">
                            <xs:annotation>
                                <xs:documentation>Power law flow transition pressure difference</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="CoefficientRelationship" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Specify how laminar and turbulent coefficents are related, if at all</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:choice>
                        <xs:element name="TransitionReynoldsNumber">
                            <xs:annotation>
                                <xs:documentation>Power law flow transition Reynolds number</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:double">
                                    <xs:minExclusive value="0.0"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="TransitionPressureDifference" type="afn:PressureQuantityType">
                            <xs:annotation>
                                <xs:documentation>Power law flow transition pressure difference</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:complexType>
            </xs:element>
            <xs:element name="Data" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Data that is/was used to determine the model parameters</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="DataPoint" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="MassFlowRate" type="afn:MassFlowRateQuantityType"/>
                                        <xs:element name="VolumeFlowRate" type="afn:VolumeFlowRateQuantityType"/>
                                    </xs:choice>
                                    <xs:element name="PressureDifference" type="afn:PressureQuantityType"/>
                                    <xs:element name="State" type="afn:AirStateType" minOccurs="0"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of power law element, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ComisDetailedOpeningType">
        <xs:sequence>
            <xs:element name="Description" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of the detailed opening element</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Coefficient">
                <xs:annotation>
                    <xs:documentation>Flow coefficient when the opening is closed</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minExclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Exponent" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Flow exponent when the opening is closed, defaults to 0.65</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="0.5"/>
                        <xs:maxInclusive value="1.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OpeningType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Type of opening, defaults to NonPivoted</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="NonPivoted"/>
                        <xs:enumeration value="HorizontallyPivoted"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="ExtraCrackLengthOrHeightOfPivotingAxis" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Extra crack length or the height of the pivoting axis [m], defaults to 0</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minExclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OpeningFactors" type="afn:OpeningFactorsType" minOccurs="2" maxOccurs="4"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of detailed opening element, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="MaterialType">
        <xs:annotation>
            <xs:documentation>A material that is transported by the flow network</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="MolarMass" type="afn:PositiveValueType" minOccurs="0"/>
            <xs:element name="Density" type="afn:DensityQuantityType" minOccurs="0"/>
            <xs:element name="SpecificHeat" type="afn:SpecificHeatQuantityType" minOccurs="0"/>
            <xs:element name="AmbientConcentration" type="afn:ConcentrationQuantityType"/>
            <xs:element name="NonTrace" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of material, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="MaterialRemovalEfficiencyType">
        <xs:annotation>
            <xs:documentation>A removal efficiency of a specific material</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="afn:PositiveUnityFractionType">
                <xs:attribute name="IDref" type="xs:IDREF" use="required">
                    <xs:annotation>
                        <xs:documentation>ID of material to be removed</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!-- Filter Elements -->
    <xs:complexType name="SimpleFilterType">
        <xs:annotation>
            <xs:documentation>Simple filter component, with only a specified removal efficiency</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Description" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of the model</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RemovalEfficiencies">
                <xs:annotation>
                    <xs:documentation>Simple filter component, with only a specified removal efficiency</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RemovalEfficiency" type="afn:MaterialRemovalEfficiencyType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of the simple filter, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!-- Sources and Sinks -->
    <xs:complexType name="ScheduledSourceType">
        <xs:annotation>
            <xs:documentation>A simple scheduled source of material</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="MaterialID">
                <xs:annotation>
                    <xs:documentation>ID of material to be added</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                </xs:complexType>
            </xs:element>
            <xs:choice>
                <xs:element name="MassScheduleID">
                    <xs:annotation>
                        <xs:documentation>ID of schedule of mass to be added</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MassFlowScheduleID">
                    <xs:annotation>
                        <xs:documentation>ID of schedule of mass flow to used to add material</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of the scheduled source, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="ConstantCoefficentSourceSinkType">
        <xs:annotation>
            <xs:documentation>A simple constant coefficient source/sink of material</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="MaterialID">
                <xs:annotation>
                    <xs:documentation>ID of material to be added</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                </xs:complexType>
            </xs:element>
            <xs:sequence>
                <xs:element name="GenerationRate">
                    <xs:annotation>
                        <xs:documentation>The source generation rate</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="afn:NonNegativeValueType">
                                <xs:attribute name="units" type="afn:MassFlowUnitsEnum" use="required"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:choice>
                <xs:element name="MassScheduleID">
                    <xs:annotation>
                        <xs:documentation>ID of schedule of mass to be added</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MassFlowScheduleID">
                    <xs:annotation>
                        <xs:documentation>ID of schedule of mass flow to used to add material</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:attribute name="IDref" type="xs:IDREF" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation>Name of the source/sink, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!-- General Model Types -->
    <xs:complexType name="AirStateType">
        <xs:sequence>
            <xs:element name="Description" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Description of the air state</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:sequence>
                    <xs:element name="Temperature" type="afn:TemperatureQuantityType" minOccurs="1"/>
                    <xs:element name="Pressure" type="afn:PressureQuantityType" minOccurs="0"/>
                </xs:sequence>
                <xs:sequence>
                    <xs:element name="Pressure" type="afn:PressureQuantityType" minOccurs="1"/>
                </xs:sequence>
            </xs:choice>
            <xs:element name="HumidityRatio" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="ID" type="xs:ID">
            <xs:annotation>
                <xs:documentation>Name of air state, must be unique</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="OpeningFactorsType">
        <xs:sequence>
            <xs:element name="OpeningFraction">
                <xs:annotation>
                    <xs:documentation>The fraction that the window or door is open</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="0.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="DischargeCoefficient" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The fractional effectiveness for air flow through the opening at this opening factor, defaults to 0.001</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minExclusive value="0.0"/>
                        <xs:maxInclusive value="1.0"/>
